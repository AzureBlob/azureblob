{
  "name": "in2it/azureblob",
  "description": "An Microsoft Azure Storage browser for blobs, tables and queues",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Michelangelo van Dam",
      "email": "michelangelo@in2it.be"
    }
  ],
  "repositories": [
    {
      "type": "pear",
      "url": "http://pear.php.net"
    }
  ],
  "require": {
    "php": ">=5.4",
    "pear-pear.php.net/mail_mime" : "^1.8",
    "pear-pear.php.net/http_request2" : "^2.2",
    "pear-pear.php.net/mail_mimedecode" : "^1.5",
    "microsoft/windowsazure": "^0.4",
    "silex/silex": "^1.2",
    "twig/twig": "^1.18",
    "monolog/monolog": "^1.13",
    "predis/predis": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.6",
    "phpspec/phpspec": "^2.2",
    "squizlabs/php_codesniffer": "^2.3",
    "fzaninotto/faker": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "AzureBlob\\": "src/"
    }
  }
}
